{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true 
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "import/no-extraneous-dependencies": 0,
        "linebreak-style":0,
        "no-underscore-dangle":"off",
        "no-unused-vars":"off",
        "no-plusplus":"off",
        "radix":"off",
        "no-restricted-syntax":"off",
        "no-param-reassign":"off",
        "no-restricted-globals":"off",
        "no-undef":"off",
        "no-prototype-builtins":"off",
        "consistent-return":"off",
        "import/prefer-default-export": "off"
    },
    "plugins": ["jasmine"]
}
